{% block settings_widget %}
    {% spaceless %}
        <div {{ block('widget_container_attributes') }}>
            {% for child in form %}
                <div class="col-md-6 col-sm-6 col-xs-6">
                    {{ form_label(child) }}
                    {{ form_widget(child) }}
                    {{ form_errors(child) }}
                </div>
            {% endfor %}
        </div>
    {% endspaceless %}
{% endblock %}