<table class="table table-bordered">
    <tbody>
    <tr class="active">
        <th>
            Название
        </th>
        <th>
            Город
        </th>
        <th>
            Страна
        </th>
        <th>

        </th>
    </tr>
    {% for team in teams %}
        <tr>
            <td>
                {{ team.getName }}
            </td>
            <td>
                {{ team.getAdminUnit.getLocality }}
            </td>
            <td>
                {{ team.getAdminUnit.getCountry }}
            </td>
            <td>
                <a href="#" class="del-team btn btn-xs btn-danger" data-x="{{ team.id }}">
                    <i class="fa fa-trash-o"></i>
                </a>
            </td>
        </tr>
    {% endfor %}

    </tbody>
</table>
